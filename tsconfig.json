{
	"compilerOptions": {
		"target": "ESNext",
		"module": "ESNext",
		"moduleResolution": "Bundler",
		"strict": true,
		"skipLibCheck": true,
		"lib": ["esnext", "dom"],
		"types": ["@cloudflare/vitest-pool-workers"],
		"jsx": "react-jsx",
		"jsxImportSource": "hono/jsx"
	},
	"include": [
		"./**/*.ts",
		"./**/*.tsx",
		"./worker-configuration.d.ts" // output of `wrangler types`
	]
}
